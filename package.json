{
  "name": "lunch",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "build": "sh ./build.sh",
    "build:watch": "tsc --watch",
    "start": "NODE_PATH=dist-current node dist-current/index.js",
    "start:dev": "NODE_PATH=src nodemon --watch src --watch .env",
    "pm2:start": "pm2 start ecosystem.json",
    "pm2:reload": "pm2 reload ecosystem.json",
    "pm2:stop": "pm2 stop ecosystem.json && pm2 delete ecosystem.json",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "test": "jest",
    "precommit": "pretty-quick --staged && npm run lint:fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "ejs": "2.6.1",
    "express": "4.16.4",
    "express-mysql-session": "2.1.0",
    "express-session": "1.16.1",
    "express-status-monitor": "1.2.5",
    "google-spreadsheet": "2.0.7",
    "helmet": "3.16.0",
    "lodash": "4.17.11",
    "morgan": "1.9.1",
    "mysql": "2.17.1",
    "node-mocks-http": "1.7.3",
    "nodemon": "1.18.11",
    "passport": "0.4.0",
    "passport-google-oauth20": "2.0.0",
    "reflect-metadata": "0.1.13",
    "routing-controllers": "0.7.7",
    "ts-node": "8.1.0",
    "typedi": "0.8.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.1",
    "@types/express": "4.16.0",
    "@types/express-session": "1.15.12",
    "@types/jest": "24.0.11",
    "@types/lodash": "4.14.119",
    "@types/mysql": "2.15.5",
    "@types/node": "10.12.15",
    "babel-jest": "24.7.1",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "pretty-quick": "1.10.0",
    "ts-jest": "24.0.2",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-security": "1.16.0",
    "typescript": "3.4.5",
    "typescript-tslint-plugin": "0.3.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "es6",
      "js",
      "ts"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "roots": [
      "<rootDir>/__test__"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts?$",
    "testEnvironment": "node"
  }
}
